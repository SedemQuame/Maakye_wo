<%- include('./partials/header.ejs');-%>
    <%- include('./partials/nav.ejs');-%>
        <!-- <p>violator_analytics &lt; admin &gt;</p> -->
        <%- include('./partials/list_filter'); -%>
            <div class="container">
                <div class="row">
                    <% if(violators.length == 0){ %>
                        <%- include('./partials/empty_list'); -%>
                            <% }else{ %>
                                <table class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">Occurences</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <%for(let i = 0; i < violators.length; i++){%>
                                            <tr>
                                                <th scope="row">
                                                    <%= (1+i) %>
                                                </th>
                                                <td>
                                                    <p>
                                                        <%= violators[i].license %>
                                                            <span class="float-right text-muted"><%= violators[i].date_created %></span> </p>
                                                    <hr>
                                                    <p class="text-muted">Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>
                                                    <a class="nav-link btn btn-outline-dark" href="/violators/<%= violators[i].video_id %>">View</a> </td>
                                            </tr>
                                            <%}%>

                                    </tbody>
                                </table>
                                <%- include('./partials/paginator'); -%>
                                    <%}%>
                </div>
            </div>

            <%- include('./partials/footer'); -%>