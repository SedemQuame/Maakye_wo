            <div class="container">
                <div class="row">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th scope="col">#</th>
                                <th scope="col">Occurences</th>
                            </tr>
                        </thead>
                        <tbody>
                            <%for(let i = 0; i < roads.length; i++){%>
